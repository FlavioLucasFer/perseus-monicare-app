  "@types/enzyme-adapter-react-16": "^1.0.6",
  "@types/jsdom": "^16.2.14",
  "@types/lodash": "^4.14.178",
  "enzyme": "^3.11.0",
  "enzyme-adapter-react-16": "^1.15.6",
  "fbjs": "^3.0.4",
  "formik": "^2.2.9",
  "jest-fetch-mock": "^3.0.3",
  "jsdom": "^19.0.0",
  "lodash": "^4.17.21",
  "moment": "^2.29.1",
  "react": "17.0.2",
